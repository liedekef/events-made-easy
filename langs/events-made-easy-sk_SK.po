msgstr "Hostiteľ SMTP. Zvyčajne to zodpovedá 'localhost'."
#
# File: eme-options.php, line: 1728
msgid "SMTP port"
msgstr "SMTP port"
#
# File: eme-options.php, line: 1728
msgid ""
